<!-- Mirrored from demo.enpek.com/html-templates/gem/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 21 Feb 2021 17:58:44 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--=================== The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags ===================-->
    <!--=================== Favicon ===================-->
    <link rel="shortcut icon" href="/images/favicon.png" type="image/png" />

    <!--=================== Description ===================-->
    <meta name="description" content="ThemeTidy.com - Choice Templates, Themes For your WebSite. Download Themes, Templates for your personal, business and commercial website. Html Templates, Themes">

    <!--=================== Title ===================-->
    <title>@ViewData["Title"] - Yash - Women fashion jewellery</title>
    <!--=================== Helpers ===================-->
    <link rel="canonical" href="index.html">

    <!--=================== Bootstrap Core CSS ===================-->
    <link href="/css/bootstrap.min.css" rel="stylesheet" type="text/css" media="all" />
    <!--=================== Paira Framework Main CSS ===================-->
    <link href="/css/paira.css" rel="stylesheet" type="text/css" media="all" />
    <!--=================== Paira Framework Main Responsive CSS ===================-->
    <link href="/css/paira-responsive.css" rel="stylesheet" type="text/css" media="all" />
    <!--=================== Font Awesome ===================-->
    <link href="/css/font-awesome.min.css" rel="stylesheet" type="text/css" media="all" />
    <!--=================== Jquery Include ===================-->
    <script src="/js/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="/js/jquery-migrate-3.0.0.min.js" type="text/javascript"></script>
    <script src="/js/jquery.easing.1.3.js" type="text/javascript"></script>

    <!--=================== Google Fonts ===================-->
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Raleway:400,600">

    <!--=================== HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries ===================-->
    <!--[if lt IE 9]>
    <script src="js/html5shiv.min.js" type="text/javascript"></script>
    <script src="js/respond.min.js" type="text/javascript"></script>
    <![endif]-->
</head>
<body>

    <!--=================== Main Body Content Container ===================-->
    <div class="paira-container">
        <!--=================== Header Container ===================-->
        <header class="header-fixed">
            <!--=================== Header Top Section ===================-->
            <section class="header-top">
                <div>
                    <div class="logo-text">
                        <a href="index.html">
                            <img src="/images/logo.png" class="img-responsive" alt="">
                        </a>
                    </div>
                    <div class="menu-wrap">
                        <a href="#" data-target="#menu-popup" data-toggle="modal" class="popup-menu">
                            <img src="/images/open.png" alt="" class="open-icon" />
                            <img src="/images/close.png" alt="" class="close-icon" />
                        </a>
                    </div>
                    <div class="cart">
                        <div class="dropdown cart-menu-body paira-cart-menu-body">
                            <a href="@Url.Action("Index","Carts")" class="padding-bottom-10"><img src="/images/cart.png" alt="" class="img-responsive center-block" /> <span class="paira-cart-item-count">@Context.Session.GetInt32("number_of_items")</span></a>
                        </div>
                    </div>
                </div>
            </section>
        </header>
        @RenderBody()
    </div>
    <!--=================== Common Message ===================-->
    <div class="modal left menu fade menu-popup" data-backdrop="static" id="menu-popup" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <h3>Menu</h3>
                            <ul class="list-unstyled nav-fix margin-top-30 margin-bottom-0">
                                <li class="active">
                                    <a href="@Url.Action("Index","Items")">Home</a>
                                    @*@Html.ActionLink("Home", "Index")*@
                                </li>
                                <li class="">
                                    <a href="@Url.Action("Index","Items", new { categoryId = 1})">RINGS</a>
                                    @*@Html.ActionLink("RINGS", "Index", new { categoryId = 1 })*@
                                </li>
                                <li class="">
                                    <a href="@Url.Action("Index","Items", new { categoryId = 2})">EARRINGS</a>
                                    @*@Html.ActionLink("EARRINGS", "Index", new { categoryId = 2 })*@
                                </li>
                                <li class="">
                                    <a href="@Url.Action("Index","Items", new { categoryId = 3})">PENDANTS</a>
                                    @*@Html.ActionLink("PENDANTS", "Index", new { categoryId = 3 })*@
                                </li>

                            </ul>
                        </div>
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <ul class="list-unstyled margin-top-30 nav-fix margin-bottom-0">
                                @if (Context.Session.GetString("name") != null)
                                {
                                    <li>
                                        <form asp-action="ChangePass" asp-controller="UsersApp" method="get">
                                            <input hidden type="number" value="@Context.Session.GetInt32("id")" name="id" />
                                            <input type="submit" value="@Context.Session.GetString("name")" class="btn btn-group-justified" />
                                        </form>
                                        @*@Html.ActionLink(@"Context.Session.GetString('name')","ChangePass","UsersApp",new { id= @Context.Session.GetInt32("id") })*@
                                    </li>
                                    <li>
                                        <form asp-action="LogOut" asp-controller="UsersApp" method="post">
                                            <input type="submit" value="Logout" />
                                        </form>
                                    </li>
                                }
                                else
                                {
                                    <li>
                                        <a asp-controller="UsersApp" asp-action="Login">Login</a>
                                    </li>
                                    <li>
                                        <a asp-controller="UsersApp" asp-action="Register">Register</a>
                                    </li>
                                }
                                <!--=================== Search Form ===================-->
                                <li>
                                    <form asp-action="Search" asp-controller="Items" method="get">
                                        <input type="text" name="name" placeholder="Items name..." required />
                                        <input type="submit" value="Search" class="btn btn-group-justified" />
                                    </form>
                                </li>
                            </ul>
                            <!--=================== Social Icons Footer ===================-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--=================== Common Message ===================-->
    <div class="modal fade video-pops" id="video-pops" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-body padding-clear">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <img src="/images/close-white.png" alt="" class="img-responsive" />
                    </button>
                    <iframe width="100" height="450" class="padding-clear embed-responsive-item" src="https://www.youtube.com/embed/6uHTHC64G48" allowfullscreen></iframe>
                </div>
            </div>
        </div>
    </div>

    <!--=================== Scripts Include ===================-->
    <script src="/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/js/jquery.easing.1.3.js" type="text/javascript"></script>
    <script src="/js/jquery.parallax-1.1.3.js" type="text/javascript"></script>
    <!--=================== Paira Framework Main Javascript ===================-->
    <script src="/js/paira.js" type="text/javascript"></script>
    @RenderSection("Scripts", required: false)
</body>

